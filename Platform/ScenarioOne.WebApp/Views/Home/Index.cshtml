@{
    ViewBag.Title = "Home Page";
}

@if (!Request.IsAuthenticated)
{
    <div class="jumbotron">
        <h1 style="text-align:center">Welcome to Web Frand</h1>
    </div>
}
else
{
    <h3 style = "color:midnightblue">Web-Fred On-Prem User Login</h3>
    <table class="table table-striped table-bordered table-hover">
        <tr><td>Name</td><td>@ViewBag.Name</td></tr>
        <tr><td>Username</td><td>@ViewBag.Username</td></tr>
        <tr><td>IP Address</td><td>@ViewBag.IpAdress</td></tr>
        @*<tr><td>Email</td><td>@ViewBag.Email</td></tr>*@
        @*<tr><td>Auth token</td><td style="word-break: break-all;">@ViewBag.AuthToken</td></tr>*@
        @*<tr><td>Session Timeout</td><td>@ViewBag.timeout</td></tr>*@
    </table>

      <br />
}

